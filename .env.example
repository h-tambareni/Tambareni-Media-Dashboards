# YouTube Data API v3 – API key from Google Cloud Console
VITE_YOUTUBE_API_KEY=your_api_key_here

# Google OAuth Client ID – for YouTube Analytics API
VITE_GOOGLE_CLIENT_ID=your_client_id_here

# ScrapeCreators API (for YouTube + TikTok)
# Optional if using Supabase: set SCRAPECREATORS_API_KEY in Edge Function secrets instead (recommended – keeps key server-side)
# VITE_SCRAPECREATORS_API_KEY=your_scrapecreators_key

# Supabase (required for Instagram OAuth + ScrapeCreators proxy)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Instagram API with Instagram Login (Business Login)
# Get from Meta Developers: https://developers.facebook.com/apps
VITE_INSTAGRAM_APP_ID=your_instagram_app_id

# App URL for OAuth redirect after Instagram connect (production)
# VITE_APP_URL=https://your-domain.com
